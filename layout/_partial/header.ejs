<header>
    <div class="wrap">
        <div id="logo">
            <a id="site-title" href="<%- url_for(config.root) %>"><%- config.title %></a>
        </div>
        <nav id="nav-header">
            <% for (var i in theme.menu) { %>
            <div class="nav-item"><a href="<%- url_for(theme.menu[i]) %>"><%- i %></a></div>
            <% } %>
        </nav>
        <div id="nav-search">
            <div class="nav-item"><a href="#"><i class="fa fa-search fa-lg" aria-hidden="true"></i></a></div>
        </div>
    </div>
</header>